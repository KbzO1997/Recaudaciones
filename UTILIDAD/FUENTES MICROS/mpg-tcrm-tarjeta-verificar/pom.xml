<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>org.springframework.boot</groupId>
        <artifactId>spring-boot-starter-parent</artifactId>
        <version>3.1.2</version>
    </parent>
    <groupId>com.bolivariano.microservice</groupId>
    <artifactId>tcrm-tarjeta-verificar</artifactId>
    <version>1.0.0</version>
    <name>tcrm-tarjeta-verificar</name>
    <description>Microservicio para la verificacion de datos de tarjeta de debito y credito</description>

    <properties>
        <java.version>17</java.version>
        <projectId>tcrm-tarjeta-verificar</projectId>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
        <maven.compiler.source>17</maven.compiler.source>
        <maven.compiler.target>17</maven.compiler.target>
        <springboot.version>3.1.2</springboot.version>
        <jacoco.version>0.7.5.201505241946</jacoco.version>
        <maven.reporting.version>2.1</maven.reporting.version>
        <slf4j.version>1.7.33</slf4j.version>
        <logback.version>1.2.9</logback.version>
        <cucumber.version>7.11.2</cucumber.version>
    </properties>
    <dependencies>

