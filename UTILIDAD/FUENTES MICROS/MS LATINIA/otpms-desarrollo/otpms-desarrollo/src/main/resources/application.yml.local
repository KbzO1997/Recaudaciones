spring:
  banner:
    location: classpath:/banner.txt       
  sybase:
    datasource:
      username:  
      password: 
      jdbcUrl: jdbc:sybase:Tds:172.16.23.22:2050             
      driver-class-name: com.sybase.jdbc4.jdbc.SybDriver
      connectionProperties: SERVER_INITIATED_TRANSACTIONS=false
      max-wait: 7000
      max-active: 30
      max-idle: 5
      #min-idle: 1
      connectionTimeout: 7000
      maximumPoolSize: 5
      minimumIdle: 0
      #initial-size: 1
      test-on-borrow: true
      #test-while-idle: true
      validation-query: "SELECT 1"
      validation-interval: 30000
      remove-abandoned: true
      remove-abandoned-timeout: 50
      time-between-eviction-runs-millis: 30000
      min-evictable-idle-time-millis: 50000
    schemas:
      cobis: cobis
      pagos: cob_pagos
