	<!-- EJECUTAR CANJE PUNTOS -->
	<xs:complexType name="MensajeEntradaEjecutarCanjePuntos">
		<xs:complexContent>
			<xs:extension base="mbl:MensajeEntrada">
				<xs:sequence>
					<xs:element name="informacionTarjeta" type="trjc:TarjetaCredito" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Datos referentes a la tarjeta de credito
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="puntosAcumulados" type="xs:integer" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>cantidad de puntos acumulados en la tarjeta de
								credito</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="puntosRedimir" type="xs:integer" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>cantidad de de puntos a redimir
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="premios" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>tasa de diferidos</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
