     <xs:element name="valorCuota" minOccurs="0" maxOccurs="1" type="xs:double" nillable="false" default="0.00">
      <xs:annotation>
       <xs:documentation>valor de la cuota, solo aplica para avances en diferido</xs:documentation>
      </xs:annotation>
     </xs:element>
     <xs:element name="tasaInteres" minOccurs="0" maxOccurs="1" type="xs:double" nillable="false" default="0.00">
      <xs:annotation>
       <xs:documentation>tasa de interes del avance, solo aplica para avances en diferido</xs:documentation>
      </xs:annotation>
     </xs:element>
    </xs:sequence>
   </xs:extension>
  </xs:complexContent>
