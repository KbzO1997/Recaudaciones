 <xs:complexType name="MensajeEntradaEjecutarAvanceEfectivo">
  <xs:complexContent>
   <xs:extension base="mbl:MensajeEntrada">
    <xs:sequence>
     <xs:element name="codigoUnicoTarjeta" type="xs:string" minOccurs="1" maxOccurs="1" nillable="false">
      <xs:annotation>
       <xs:documentation>codigo unico de la tarjeta de credito</xs:documentation>
      </xs:annotation>
     </xs:element>
     <xs:element name="codigoSeguridad" type="xs:string" minOccurs="1" maxOccurs="1" nillable="false">
      <xs:annotation>
       <xs:documentation>codigo de seguridad encriptado</xs:documentation>
      </xs:annotation>
     </xs:element>
     <xs:element name="fechaCaducidad" type="xs:integer" minOccurs="1" maxOccurs="1" nillable="false">
      <xs:annotation>
       <xs:documentation>fecha de caducidad de la tarjeta de credito</xs:documentation>
      </xs:annotation>
     </xs:element>
     <xs:element name="tipoCuentaDebito" minOccurs="1" maxOccurs="1" type="en:TipoCuenta" nillable="false">
      <xs:annotation>
       <xs:documentation>tipo de cuenta de debito</xs:documentation>
      </xs:annotation>
     </xs:element>
     <xs:element name="cuentaDebito" minOccurs="1" maxOccurs="1" type="xs:string" nillable="false">
      <xs:annotation>
       <xs:documentation>cuenta de debito</xs:documentation>
      </xs:annotation>
     </xs:element>
     <xs:element name="subproducto" minOccurs="0" maxOccurs="1" type="en:Subproducto" nillable="false">
      <xs:annotation>
       <xs:documentation>nemonico de subproducto</xs:documentation>
      </xs:annotation>
     </xs:element>
     <xs:element name="monto" minOccurs="1" maxOccurs="1" type="xs:double" nillable="false">
      <xs:annotation>
       <xs:documentation>monto del avance</xs:documentation>
      </xs:annotation>
     </xs:element>
     <xs:element name="modoPago" minOccurs="1" maxOccurs="1" type="en:TipoConsumoTarjetaCredito" nillable="false">
      <xs:annotation>
       <xs:documentation>modo de pago (DIFERIDO - CORRIENTE)</xs:documentation>
      </xs:annotation>
     </xs:element>
     <xs:element name="plazo" minOccurs="0" maxOccurs="1" type="xs:integer" nillable="false" default="0">
      <xs:annotation>
       <xs:documentation>plazo en meses del avance, solo aplica para avances en diferido</xs:documentation>
