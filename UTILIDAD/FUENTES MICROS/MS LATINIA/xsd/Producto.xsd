				</xs:annotation>
			</xs:element>
			<xs:element name="codigoEmpresa" type="xs:int" minOccurs="0" maxOccurs="1"/>
			<xs:element name="codigoLinea" type="xs:int" minOccurs="0" maxOccurs="1">
