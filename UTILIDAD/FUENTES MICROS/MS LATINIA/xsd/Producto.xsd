				<xs:annotation>
					<xs:documentation>codigo detalle de cupo</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="disponibleEmpresa" type="xs:float" minOccurs="0" maxOccurs="1"/>
