<soap-env:messages xmlns:soap-env="http://schemas.xmlsoap.org/soap/envelope/">
	<message key="async.request">
		<soap-env:Body>
			<SOAP-ENV:Envelope
				xmlns:SOAP-ENV="http://www.w3.org/2003/05/soap-envelope"
				xmlns:e="http://www.technisys.net/cmm/services/errors/v1.0"
				xmlns:wsu="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-utility-1.0.xsd"
				xmlns:wsse="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-secext-1.0.xsd"
				xmlns:md="http://www.technisys.net/cmm/services/metadata/v2.0">
				<SOAP-ENV:Header>
					<md:metadata>
						<internals/>
						<serviceId>modifyMonetaryTransactionByExecution</serviceId>
						<executingOperatorId>${executingOperatorId}</executingOperatorId>
						<cmmDispatchDate>${fechaEjecucion}</cmmDispatchDate>
						<customProperties>
							<isPersonalBank type="java.lang.Boolean">${canalLogo}</isPersonalBank>
						</customProperties>
					</md:metadata>
				</SOAP-ENV:Header>
				<SOAP-ENV:Body>
					<srv:modifyMonetaryTransactionByExecutionRequest
						xmlns:srv="http://www.technisys.net/cmm/services/modifyMonetaryTransactionByExecution/rq/v0.0">
						<generic>
							<offline type="java.lang.Boolean">${offline}</offline>
						</generic>
						<collection name="transactionSets">
							<transactionSet>
								<commission>${comision}</commission>
								<executionDate>${fechaEjecucion}</executionDate>
								<processDate>${fechaProceso}</processDate>
								<sourceDataTransaction>
									<customerOperation>
										<currency>
											<shortDesc>${nemonicoMonedaOrigen}</shortDesc>
										</currency>
										<operationIdMasked>${cuentaEnmascaradaOrigen}</operationIdMasked>
										<type>
											<shortDesc>${tipoCuentaOrigen}</shortDesc>
										</type>
										<customer>
											<name>${nombreOrdenante}</name>
										</customer>
									</customerOperation>
								</sourceDataTransaction>
								<concept>${concepto}</concept>
								<transactionType>
									<mnemonic>${tipoPago}</mnemonic>
								</transactionType>
								<transferType>
									<mnemonic>PAGO_TARJETA_TERCEROS</mnemonic>
								</transferType>
								<targetDataTransactions>
									<targetDataTransaction>
										<amount>${monto}</amount>
										<currency>
											<shortDesc>USD</shortDesc>
										</currency>
										<customerOperation>
											<operationIdMasked>${tarjetaEnmascarada}</operationIdMasked>
											<type>
												<shortDesc>${descripcionCategoriaTarjeta}</shortDesc>
											</type>
											<customer>
												<name>${titularTarjeta}</name>
												<alias>${aliasBeneficiario}</alias>
												<customerId>${idEnmascaradoBeneficiario}</customerId>
												<identificationType>
													<shortDesc>${tipoIdBeneficiario}</shortDesc>
												</identificationType>
											</customer>
											<product>
												<bank>
													<shortDesc>${bancoBeneficiario}</shortDesc>
												</bank>
											</product>
										</customerOperation>
									</targetDataTransaction>
								</targetDataTransactions>
			          <operatorMail>
			              <emailAddressComplete>{correoBeneficiario}</emailAddressComplete>
			          </operatorMail>
								<transactionSetId>${referencia}</transactionSetId>
							</transactionSet>
						</collection>
						<processTransferType name="processTransferType">
							<shortDesc>${viaDelPago}</shortDesc>
						</processTransferType>
					</srv:modifyMonetaryTransactionByExecutionRequest>
				</SOAP-ENV:Body>
			</SOAP-ENV:Envelope>
		</soap-env:Body>
	</message>
	<message key="async.response">
		<soap-env:Body>
			<Envelope>
				<Header/>
				<Body/>
			</Envelope>
		</soap-env:Body>
	</message>
</soap-env:messages>