Sub txtCampo_KeyPress(Index As Integer, KeyAscii As Integer)

    Select Case Index%
        Case 0, 1, 2, 4, 7, 8
           KeyAscii% = FMValidaTipoDato("N", KeyAscii%)
        Case 3
            KeyAscii% = FMValidaTipoDato("B", KeyAscii%)
        Case 5
            KeyAscii% = Asc(UCase$(Chr$(KeyAscii%)))
        Case 6
            KeyAscii% = FMValidaTipoDato("A", KeyAscii%)
        Case 9
            KeyAscii% = FMValidaTipoDato("A", KeyAscii%)
    End Select

End Sub

