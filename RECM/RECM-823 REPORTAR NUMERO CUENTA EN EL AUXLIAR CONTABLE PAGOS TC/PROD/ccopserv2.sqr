ts_tipo_chequera    &tipo_chequera  !Ref007:msilvag
ts_descripcion_ec		    !Ref10 pholguiv
isnull(ts_cta_banco_dep,'')    &ts_cta_banco_dep  !PHOYOSV 20180607 REF_11
isnull(ts_cta_asociada,ts_cta_banco)	&ts_cta_asociada  !ref16 !ref 18
isnull(ts_cta_banco,ts_cta_banco)	&ts_cta_banco  !Ref 19 KBastida TC ANT
isnull(ts_tarjcred,'')	&ts_tarjcred  !Ref 22
isnull(ts_plazo,'')		&ts_plazo	!ref16
ts_fonres_iess !ref 25


    !ref20 inicio
    if (&ts_tipo_transaccion = 3142 or &ts_causa = '18786') !lcepeda -  KBastida 27
        if (&ts_tarjcred <>'')
            let $ts_tarjcred = substr(&ts_tarjcred,7,7) !lcepeda
        end-if
    else
        let $ts_cta_asociada = substr(&ts_cta_asociada,7,7)  !ref16 !ref 25
    end-if
    !ref20 fin
   !let $ts_cta_asociada = substr(&ts_cta_asociada,7,10)  !ref16 !ref20
   let $ts_cta_banco 	= substr(&ts_cta_banco,7,10)  !Ref 18 KBastida TC ANT
   
   let $ts_plazo = &ts_plazo   !ref16
   let $ts_cta_banco_dep = &ts_cta_banco_dep      !PHOYOSV 20180607 REF_11
 evaluate &ts_tipo_transaccion
  when = 3028
    let $canal = &canal
    break
  when = 3029
    let $canal = &canal
    break
  when = 3507
    let $canal = &canal
    break
  when = 3014     !JPM  Ref 6-->
    let $canal = &canal
    break
  when = 3545
    let $canal = &canal
    break
  when = 3738
    let $canal = &canal
    break
  when = 3937
    let $canal = &canal
    break         !<-- JPM Ref 6
  when-other
    let $canal = ''
    break
 end-evaluate
 !<--- JPM Ref. 5
 if &ts_correccion = 'S'       !* DSC980112 *!
    let #factor = -1
 else
    let #factor = 1
 end-if                        !* DSC980112 *!

 let $canal_trx = &tipo_chequera    !Ref007:msilvag

 let #quiebre = 0
 do Imprime_Registro !*** IMPRIME LINEA DE DETALLE ***!

 next-listing
 do Actualizar_Reg_Proc
from  cc_tran_servicio_resp, cobis..cl_ttransaccion, cc_ctacte, cobis..cl_oficina !OJO REGRESAR REF_12
where ts_tipo_transaccion *= tn_trn_code
  and of_filial           = convert(tinyint,#i_filial)
  and ts_oficina          = of_oficina
  and cc_cta_banco        =* isnull(ts_cta_banco,ts_cta_banco_dep)
  and ts_tipo_transaccion not in (43545, 43549)     !ref9 pricaura cambio por primax !ref24 domiciliados bb

 
order by ts_oficina, ts_moneda,ts_usuario,ts_tipo_transaccion,ts_cta_banco,ts_secuencial
End-Select
