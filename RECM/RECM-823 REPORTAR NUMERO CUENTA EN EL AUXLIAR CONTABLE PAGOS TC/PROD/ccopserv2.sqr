Begin-Procedure Imprime_Registro
  let #cheqe = 0

  if (&ts_tipo_transaccion > 10401 and &ts_tipo_transaccion < 10405) or
     (&ts_tipo_transaccion > 10461 and &ts_tipo_transaccion < 10465)
     do PrintS(1,'CORRESPONSAL','',0)
     do PrintS(2,&ts_corresponsal,'',0)
     let #valor = &ts_saldo
  else
  if (&ts_tipo_transaccion = 3070 or &ts_tipo_transaccion = 3328)
     do PrintS(1,'CAUSA:','',0)
     do PrintS(2,&ts_causa,'',0)
     let #valor = &ts_aporte_iess          !* EFECTIVO
     let #cheqe = &ts_descuento_iess       !* CHEQUES
  else
    if (&ts_tipo_transaccion = 3032 or &ts_tipo_transaccion = 3113 or &ts_tipo_transaccion = 3335)   !Ref08 pholguiv --agrega trx diferida 3335 !* RE 19990408 *!
       do PrintS(1,'CAUSA:','',0)
       do PrintS(2,&ts_causa,'',0)
       let #valor = &ts_saldo       !* EFECTIVO
       let #cheqe = &ts_valor       !* CHEQUES
    else
    if (&ts_tipo_transaccion = 3142 or &ts_tipo_transaccion = 3141 or &ts_tipo_transaccion = 3137
            or &ts_tipo_transaccion = 3121 or &ts_tipo_transaccion = 3168 or &ts_tipo_transaccion = 3169 )    !*** KFAH:10may2000  ***!
       !ref20 inicio
       !do PrintS(1,'referencia:','',0)
       !do PrintS(2,&ts_referencia,'',0)
       !let #valor = &ts_saldo       !* EFECTIVO
       !let #cheqe = &ts_valor       !* CHEQUES
       
       if (&ts_monto > 0)!lcepeda
          do PrintS(1,$ts_tarjcred,'',0)
		  do PrintS(2,&cc_nombre,'',0)
          !do PrintS(1,'referencia:','',0)
          !do PrintS(2,&cc_nombre,'',0)
            let #valor = &ts_monto       !* EFECTIVO
            let #cheqe = &ts_valor       !* CHEQUES
       else
        if (&ts_ocasional > 0)
            do PrintS(1,&cta_banco,'',0)
            do PrintS(2,&cc_nombre,'',0)
            let #valor = &ts_saldo   !* EFECTIVO
            let #cheqe = &ts_valor       !* CHEQUES
        else
            do PrintS(1,'referencia:','',0)
            do PrintS(2,&ts_referencia,'',0)
            let #valor = &ts_saldo       !* EFECTIVO
            let #cheqe = &ts_valor       !* CHEQUES
        end-if
       end-if
       !ref20 Fin
    else
    !Ref007:msilvag Inicio
    if (&ts_tipo_transaccion = 43335  and $canal_trx = 'CNB') or (&ts_tipo_transaccion = 43337  and $canal_trx = 'CNB' ) or &ts_tipo_transaccion = 30166 or &ts_tipo_transaccion = 30168 ! REF 26 Nuevas Transacciones 
        let #valor = &ts_monto
    else
    !Ref007:msilvag Fin
    if (&ts_tipo_transaccion = 10431)    !*** KFAH:10may2000  ***!
       do PrintS(1,'referencia:','',0)
       do PrintS(2,&ts_corresponsal,'',0)
       let #valor = &ts_valor       !* EFECTIVO
       let #cheqe = &ts_monto       !* CHEQUES
    else
    if (&ts_tipo_transaccion = 3208)    !*** ESA 17/NOV/2005 ***!
  let $numero =  to_char(&ts_cheque_rec)
  let $premio = &ts_autoriz_aut
  let $cod_giro = &ts_referencia
  let $referencia = $cod_giro || ' - ' || $numero || ' - ' || $premio
       do PrintS(1,'referencia:','',0)
       do PrintS(2,$referencia,'',0)
       if &ts_valor = 0
          let #valor = &ts_saldo
       else
          let #valor = &ts_valor
       end-if
    else
    if (&ts_tipo_transaccion = 3173)    !*** RE 11/OCT/2000 ***!
       do PrintS(1,'referencia:','',0)
       do PrintS(2,&ts_referencia,'',0)
       let #valor = &ts_valor                                   !* EFECTIVO
       let #cheqe = &ts_saldo + &ts_monto + &ts_ocasional       !* CHEQUES-ND-RETENCION
    else
    !ref 15 ini
      if (&ts_tipo_transaccion = 43569)   
        do PrintS(1,&cta_banco,'XXX-XXXXXXX',0)
        do PrintS(2,&cc_nombre,'',0)
      	let #cheqe = &ts_saldo
        let #valor = &ts_valor     
      else
    !ref 15 fin
    !ref 16 ini
	if ((&ts_causa = '7749' or &ts_causa = '1417' or &ts_causa = '1430' or &ts_causa = '2235' or &ts_causa = '2237' or &ts_causa = '2319') and (&ts_plazo = ' ')) !ref 19
	  do PrintS(1,&cta_banco,'XXX-XXXXXXX',0)
	  do PrintS(2,&cc_nombre,'',0)
	  let #cheqe = &ts_saldo
	  let #valor = &ts_valor     
	else
	if ((&ts_causa = '7749' or &ts_causa = '1417' or &ts_causa = '1430' or &ts_causa = '2235' or &ts_causa = '2237' or &ts_causa = '2319') and &ts_plazo <> ' ') !ref 19
		  do PrintS(1,$ts_cta_asociada,'',0)
		  do PrintS(2,&cc_nombre,'',0)
		  let #cheqe = &ts_saldo
		  let #valor = &ts_contratado     
	else
    !ref 16 fin
       do PrintS(1,&cta_banco,'',0) !ref 25
       do PrintS(2,&cc_nombre,'',0)
       if &ts_valor = 0
          let #valor = &ts_saldo
	  
  else  
    let #valor = &ts_valor
       end-if
    end-if	
    end-if
    end-if
    end-if
    end-if !Ref007:msilvag
    end-if
    end-if
  end-if
  end-if
  end-if
  end-if


	!Ref 18 KBastida TC ANT
	if (&ts_causa = '18786' and &ts_contratado = 0) 
	  do PrintS(1,&cta_banco,'XXX-XXXXXXX',0)
	  do PrintS(2,&cc_nombre,'',0)
	  let #cheqe = &ts_saldo
	  let #valor = &ts_valor     
	else	
	if (&ts_causa = '18786' and &ts_contratado <> 0) 
		  do PrintS(1,$ts_tarjcred,'XXXXXXX    ',0)  !KBastida 27
		  do PrintS(2,&cc_nombre,'',0)
		  let #cheqe = &ts_saldo
		  let #valor = &ts_contratado     	
		!else  
        !do PrintS(1,&cta_banco,'XXX-XXXXXXX',0)
        !do PrintS(2,&cc_nombre,'',0)
        !if &ts_valor = 0
        !   let #valor = &ts_saldo
		!	end-if 
    end-if 
	end-if   
	!Ref 18 Fin
    
    !ref 25 lcepedaf
        if (&ts_tipo_transaccion = 3611 )
            if (&ts_fonres_iess > 0) 
              do PrintS(1,$ts_cta_banco,'XXXXXXX    ',0)
              do PrintS(2,&cc_nombre,'',0)
              let #valor  = &ts_fonres_iess
              let #cheqe = &ts_valor
            else
              if (&ts_aporte_iess > 0)
                do PrintS(1,$ts_cta_banco,'',0)
                do PrintS(2,&cc_nombre,'',0)
                !let #cheqe = &ts_aporte_iess
                let #valor = &ts_valor
              !else
               ! do PrintS(1,&ts_cta_banco,'',0)
               ! do PrintS(2,&cc_nombre,'',0)
                !let #cheqe = &ts_saldo
                !let #valor = &ts_valor
              end-if
            end-if   
        end-if 
    !Ref 25 lcepeda Fin
	
  !Ref 21 KBastida Claro SrvFijos Cheque
    if &ts_causa = '8521' or  &ts_causa = '9718' !Ref 23
		if &ts_cheque_rec <> 0
			let #cheqe = &ts_saldo	
			let #valor = &ts_valor
		end-if  
  end-if  
  
  
  if &ts_causa = '9718' !Ref 23
		if &ts_cheque_rec <> 0
			let #cheqe = &ts_valor	
			let #valor = &ts_saldo
		end-if  
  end-if  
  
  !Ref 21 Fin
	

  do PrintN(3,#valor,'{FOR_DIN}',0)
  do PrintN(4,#cheqe,'{FOR_DIN}',0)
  
  !ini ref16
  if ((&ts_causa = '7749' or &ts_causa = '1417' or &ts_causa = '1430' or &ts_causa = '2235' or &ts_causa = '2237' or &ts_causa = '2319') and (&ts_cta_asociada <> ' ' or &cta_banco <> ' ')) !ref 19
  	if &ts_plazo <> ' '
  		do PrintS(5,'DIF','',0)
  	else
  		do PrintS(5,'EFE','',0)
  	end-if
  else
  	do PrintS(5,&ts_clase,'',0)
  end-if
  !fin ref16
  
  !ini ref 22
  if ((&ts_causa = '5993' or &ts_causa = '2') and (&cta_banco <> ' ' ))  !ref21
    if &ts_monto > 0 and &ts_tipo_transaccion = 3142 and &ts_tarjcred <>''
  		do PrintS(5,'DIF','',0)
        do PrintS (13,'TARJETA','',0)
    end-if
  end-if
  !fin ref 22
  
  !ini ref 25 lcepeda
    if ((&ts_causa = '0') and (&cta_banco <> ' ' ))  
    if &ts_fonres_iess > 0 and &ts_tipo_transaccion = 3611 and &ts_plazo ='1'
        do PrintS(5,'ROT','',0)
        do PrintS (13,'TARJETA','',0)
    else
        if (&ts_fonres_iess > 0 and &ts_tipo_transaccion = 3611)
            do PrintS(5,'DIF','',0)
            do PrintS (13,'TARJETA','',0)
        end-if
    end-if
    end-if
   !fin ref 25 lcepeda
  
  !Ref 18 KBastida TC ANT
  
  
  
  
  if &ts_causa = '18786'  and &ts_tarjcred <> ''  !KBastida 27
  	if &ts_contratado <> 0
  		do PrintS(5,'DIF','',0)
  	else 
		do PrintS(5,&ts_clase,'',0)    !KBastida 27
  	end-if
  else
  	do PrintS(5,&ts_clase,'',0)
  end-if
  !Ref 18 Fin

  let $w_causa = '  '|| &ts_causa
  do PrintS(6,$w_causa,'',0)
  if &ts_endoso > 0
     do PrintN(7,&ts_endoso, '{FOR_CHQ}', 0)
  end-if
  if &ts_cheque > 0
     do PrintN(7,&ts_cheque, '{FOR_CHQ}', 0)
  end-if
  if &ts_cheque_desde > 0
     do PrintN(8,&ts_cheque_desde, '{FOR_CHQ}', 0)
  end-if
  if &ts_cheque_hasta > 0
     do PrintN(9,&ts_cheque_hasta, '{FOR_CHQ}', 0)
  end-if
  let #suma_total = #suma_total + (#valor * #factor)   !* DSC980112 *!
  let #cheq_total = #cheq_total + (#cheqe * #factor)   !* DSC980112 *!
  let #total      = #valor      + #cheqe

  do PrintS (10,&hora_transa,'',0)
  do PrintS (11,&ts_terminal,'',0)
  do PrintS (12,$canal,'',0)             !JPM Ref 5

    !--Ref10 valida corresponsal--
  if (&ts_tipo_transaccion = 3875)
       do Corresponsal
       do PrintS (13,&corresponsal,'',0)
  end-if
  if &ts_tipo_transaccion = 3188 and &ts_causa = '10'   !PHOYOSV 20180607 INI REF_05
   do PrintS (13,$ts_cta_banco_dep,'',0)
  end-if                                                !PHOYOSV 20180607 FIN REF_05
  
  if ((&ts_causa = '7749' or &ts_causa = '1417' or &ts_causa = '1430' or &ts_causa = '2235' or &ts_causa = '2237' or &ts_causa = '2319' or &ts_causa = '18786') and (&ts_plazo <> ' '))  !ref16 !ref 19 !KBastida 27
    do PrintS (13,'TARJETA','',0)
  end-if
  

  if &ts_correccion = 'S'                !* DSC980112 *!
     do PrintS (14,'*','',0)  !JPM Ref 5: antes col 12
     let #total = #total * -1
     let #reg   = -1
  else
     do PrintS (14,' ','',0)  !JPM Ref 5: antes col 12
     let #reg   = 1
  end-if                                 !* DSC980112 *!

!**************** LEE EN re_transa (DSC971013)
let #existe = 0
Begin-Select
tr_tipo_tran &t
  let #existe = 1
from cob_remesas..re_transa_cs
where tr_tipo_tran = &ts_tipo_transaccion
  and tr_causa     = &ts_causa
End-Select


if #existe = 1
   do Actualiza_Tabla
else
   do Inserta_Tabla
end-if
!**************** (DSC971013)
End-Procedure

Begin-Procedure Actualiza_Tabla
!------------------------------
begin-sql
  update cob_remesas..re_transa_cs
  set tr_valor = tr_valor + #total, tr_contador = tr_contador + #reg   !* DSC
  where tr_tipo_tran = &ts_tipo_transaccion
    and tr_causa     = &ts_causa
end-sql
End-Procedure

Begin-Procedure Inserta_Tabla
!------------------------------
Begin-Select
tn_descripcion &descripcion
from cobis..cl_ttransaccion
where tn_trn_code = &ts_tipo_transaccion
End-Select
begin-sql
   insert into cob_remesas..re_transa_cs
   values(null,&ts_tipo_transaccion,null,&ts_causa,&descripcion,1,#total)
end-sql
End-Procedure

Begin-Procedure Before_Transa
  let $codigo_transa = '(' || to_char(&tn_trn_code) || ')'
  position (+1)
  do PrintS (1,'TRANSACCION:','',0)
  do PrintS (2,&tn_descripcion,'',0)
  do PrintS (3,$codigo_transa,'',0)
  position (+1)
End-Procedure

Begin-Procedure After_Transa
  !* Imprime los totales por transaccion *!
  do PrintS (1,'TOTALES:','',0)
  do PrintN (3,#suma_total,'{FOR_DIN}',0)
  do PrintN (4,#cheq_total,'{FOR_DIN}',0)
  let #suma_total = 0
  let #cheq_total = 0
  position (+1)
End-Procedure


